<li class="speaker">
	<div class="speaker-pic">
		<figure>
			<a href="#"><img src="<%- img %>" alt="<%- name %>" /></a>
		</figure>
		<a href="<%- twitter %>" class="twitter"></a>
		<a href="<%- linkedin %>" class="linkedin"></a>
	</div>
	<p class="speaker-name"><a href="#"><%- name %></a></p>
	<p class="speaker-position"><%- position %></p>
	<p class="speaker-company"><%- company %></p>
</li>